<h1>Listing calendars</h1>

<table>
  <thead>
    <tr>
      <th>New event title</th>
      <th>Location</th>
      <th>Date</th>
      <th>Starts</th>
      <th>Ends</th>
      <th>Notes</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @calendars.each do |calendar| %>
      <tr>
        <td><%= calendar.New_Event_Title %></td>
        <td><%= calendar.Location %></td>
        <td><%= calendar.Date %></td>
        <td><%= calendar.Starts %></td>
        <td><%= calendar.Ends %></td>
        <td><%= calendar.Notes %></td>
        <td><%= link_to 'Show', calendar %></td>
        <td><%= link_to 'Edit', edit_calendar_path(calendar) %></td>
        <td><%= link_to 'Destroy', calendar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar', new_calendar_path %>
